xableWarningCount":0,"source":"129"},{"filePath":"130","messages":"131","suppressedMessages":"132","errorCount":0,"fatalErrorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"92"},{"filePath":"133","messages":"134","suppressedMessages":"135","errorCount":0,"fatalErrorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"69"},{"filePath":"136","messages":"137","suppressedMessages":"138","errorCount":0,"fatalErrorCount":0,"warningCount":2,"fixableErrorCount":0,"fixableWarningCount":0,"source":"88"},{"filePath":"139","messages":"140","suppressedMessages":"141","errorCount":0,"fatalErrorCount":0,"warningCount":2,"fixableErrorCount":0,"fixableWarningCount":0,"source":"76"},"/Users/diegolaya/Desktop/Projects/theraspanishplay/theraspanishplay/src/index.js",[],[],"/Users/diegolaya/Desktop/Projects/theraspanishplay/theraspanishplay/src/App.js",["142"],[],"import logo from './logo.svg';\nimport './App.css';\nimport { useState } from 'react';\nimport { BrowserRouter as Router } from 'react-router-dom';\nimport Choreograph from './choreograph/Choreograph.js';\nimport Navigation from './components/navigation/Navigation.js';\nimport Menu from './components/menu/Menu.js';\n\nfunction App() {\n\n  const [showMenu, setShowMenu] = useState(false);\n\n  function toggleMenu() {\n    setShowMenu(!showMenu)\n  }\n\n  return (\n    <div>\n      <Router>\n          {\n            showMenu && <Menu toggleMenu={toggleMenu}/>\n          }\n          <Navigation toggleMenu={toggleMenu}/>\n          <Choreograph />\n      </Router>\n    </div>\n  );\n}\n\nexport default App;\n","/Users/diegolaya/Desktop/Projects/theraspanishplay/theraspanishplay/src/reportWebVitals.js",[],[],"/Users/diegolaya/Desktop/Projects/theraspanishplay/theraspanishplay/src/components/menu/Menu.js",["143"],[],"import \"./Menu.css\"\nimport \"../../App.css\"\nimport { Link, useHistory } from 'react-router-dom';\n\nconst Menu = (toggleMenu) => {\n    return (\n        <div id=\"menu-background\">\n            <h5 id='close' onClick={toggleMenu.toggleMenu}>X CLOSE</h5>\n            <div id=\"options\">\n                <Link onClick={toggleMenu.toggleMenu} to=\"/about\"  title=\"About\">\n                    <h4>ABOUT</h4>\n                </Link>\n                <Link onClick={toggleMenu.toggleMenu} to=\"/program\"  title=\"Program\">\n                    <h4>PROGRAM</h4>\n                </Link>\n                <Link onClick={toggleMenu.toggleMenu} to=\"/proyecto-play\"  title=\"ProyectoPlay\">\n                    <h4>PROYECTO PLAY</h4>\n                </Link>\n                <Link onClick={toggleMenu.toggleMenu} to=\"/proyecto-play/about\"  title=\"About\">\n                    <h5>ABOUT</h5>\n                </Link>\n                <Link onClick={toggleMenu.toggleMenu} to=\"/proyecto-play/past-events\"  title=\"PastEvents\">\n                    <h5>PAST EVENTS</h5>\n                </Link>\n                <Link onClick={toggleMenu.toggleMenu} to=\"/proyecto-play/upcoming-events\"  title=\"UpcomingEvents\">\n                    <h5>UPCOMING EVENTS</h5>\n                </Link>\n                <Link onClick={toggleMenu.toggleMenu} to=\"/resources\"  title=\"Resources\">\n                    <h4>RESOURCES FOR PLAY</h4>\n                </Link>\n                <Link onClick={toggleMenu.toggleMenu} to=\"/press\"  title=\"Press\">\n                    <h4>PRESS</h4>\n                </Link>\n            </div>\n        </div>\n    );\n\n}\n\nexport default Menu;","/Users/diegolaya/Desktop/Projects/theraspanishplay/theraspanishplay/src/components/program/Program.js",["144","145"],[],"/Users/diegolaya/Desktop/Projects/theraspanishplay/theraspanishplay/src/components/footer/Footer.js",["146","147"],[],"import \"./Footer.css\"\nimport \"../../App.css\"\nimport { Link, useHistory } from 'react-router-dom';\n\nconst Footer = () => {\n    return (\n        <div>\n        </div>\n    );\n}\n\nexport default Footer;",[],"/Users/diegolaya/Desktop/Projects/theraspanishplay/theraspanishplay/src/components/resources/Resources.js",["148","149","150"],[],"/Users/diegolaya/Desktop/Projects/theraspanishplay/theraspanishplay/src/components/proyecto-play/ProyectoPlay.js",["151"],[],"import React, { useEffect, useRef, useState } from 'react';\nimport '../../App.css';\nimport './ProyectoPlay.css';\nimport Footer from '../footer/Footer.js';\n// import '../press/Press.css'\nimport { ReactComponent as ProyectoPlayLogo } from '../../assets/navigation-assets/ProyectoPlayLogo.svg';\n\nimport { ReactComponent as MissingImage } from '../../assets/navigation-assets/MissingImage.svg';\n\nimport { ReactComponent as FamilyDay } from '../../assets/navigation-assets/FamilyDay.svg';\nimport { ReactComponent as FamilyDay1 } from '../../assets/navigation-assets/FamilyDay1.svg';\nimport { ReactComponent as FamilyDay2 } from '../../assets/navigation-assets/FamilyDay2.svg';\nimport { ReactComponent as FamilyDay3 } from '../../assets/navigation-assets/FamilyDay3.svg';\n\nimport { ReactComponent as CounselingCenter } from '../../assets/navigation-assets/CounselingCenter.svg';\nimport { ReactComponent as CounselingCenter1 } from '../../assets/navigation-assets/CounselingCenter1.svg';\nimport { ReactComponent as CounselingCenter2 } from '../../assets/navigation-assets/CounselingCenter2.svg';\nimport { ReactComponent as CounselingCenter3 } from '../../assets/navigation-assets/CounselingCenter3.svg';\n\n\nconst scrollToRef = (ref) => window.scrollTo(0, ref.current.offsetTop)\n\nconst ProyectoPlay = () => {\n\n    const pastEventsRef = useRef(null);\n\n    const [seePhotosFamilyDay, setSeePhotosFamilyDay] = useState(false);\n    const [seePhotosCounselingCenter, setSeePhotosCounselingCenter] = useState(false);\n\n    const scrollToPastEvents = () => {\n        scrollToRef(pastEventsRef);\n    }\n\n    return (\n        <div className='cream unconstrained-portion'>\n            <div id=\"content\">\n                <ProyectoPlayLogo id=\"proyecto-play-logo\"/>\n                <div className='horizontal-stack'>\n                    <div className='orange-title'>WHAT IS IT</div>\n                </div>\n                <div className='horizontal-stack'>\n                    <div id=\"what-is-it-text\">\n                        This project focuses on hosting large scale\n                        community events for parents and children to\n                        get together and play traditional games.\n                    </div>\n                    <div className='lead-stack'>\n                        <div id='class-type-parent'>\n                            <button onClick={()  => { window.open('mailto:theraspanishplay@gmail.com?subject=Partner%20Request&body=')}} class=\"button purple\">\n                                <p>partner with proyecto play</p>\n                            </button>\n                            <button onClick={scrollToPastEvents} class=\"button green\">\n                                <p>see events</p>\n                            </button>\n                        </div>\n                        <p id=\"paragraph\">\n                            In the age of electronics we live in, video games\n                            can take over a child’s free time. This is the\n                            reason why it is so important to be intentional\n                            about creating opportunities where children can\n                            play freely, use their imaginations, and have fun.\n                            Proyecto Play main goal is to create those moments\n                            where kids can interact with their parents playing\n                            traditional games and spending quality time free\n                            of any electronic distraction.\n                        </p>\n                    </div>\n                </div>\n                <div id=\"events-container\">\n                    <h1 ref={pastEventsRef} className=\"press-title\">UPCOMING EVENTS</h1>\n                    <div className=\"press-releases-container\">\n                         <div className=\"press-article\">\n                            <div className=\"press-core\">\n                                <MissingImage className=\"press-image\"/>\n                                <div className=\"press-info-container\">\n                                    <h4 className=\"press-date\">\n                                        <b>COMING SOON</b>\n                                    </h4>\n                                    <h4 className=\"press-info\">\n                                        KUWAIT NATIONAL LIBRARY\n                                    </h4>\n                                </div>\n                            </div>   \n                        </div>\n                    </div>\n                    <h1 className=\"press-title\">PAST EVENTS</h1>\n                    <div className=\"press-releases-container\">\n                        <div className=\"press-article\">\n                            <div className=\"press-core\">\n                                <FamilyDay className=\"press-image\"/>\n                                <div className=\"press-info-container\">\n                                    <h4 className=\"press-date\">\n                                        <b>January, 2020</b>\n                                    </h4>\n                                    <h4 className=\"press-info\">\n                                        DAI KUWAIT, KUWAIT CITY\n                                    </h4>\n                                    <h4 className=\"press-info\">\n                                        FAMILY DAY\n                                    </h4>\n                                </div>\n                            </div>\n                            <button onClick={() => {setSeePhotosFamilyDay(!seePhotosFamilyDay)}} className=\"see-photos-button\">\n                                {seePhotosFamilyDay ? \"x close photos\" : \"> see photos\"}\n                            </button>\n                           \n                        </div>\n                        {\n                                seePhotosFamilyDay &&\n                                    <div className='photos'>\n                                        <FamilyDay1 className='collection-image'/>\n                                        <FamilyDay2 className='collection-image'/>\n                                        <FamilyDay3 className='collection-image'/>\n                                    </div>\n                            }\n\n                        <div className=\"press-article\">\n                            <div className=\"press-core\">\n                                <CounselingCenter className=\"press-image\"/>\n                                <div className=\"press-info-container\">\n                                    <h4 className=\"press-date\">\n                                        <b>March, 2019</b>\n                                    </h4>\n                                    <h4 className=\"press-info\">\n                                        AL HAMRA, KUWAIT CITY\n                                    </h4>\n                                    <h4 className=\"press-info\">\n                                        KUWAIT COUNSELING CENTER\n                                    </h4>\n                                </div>\n                            </div>\n                            <button onClick={() => {setSeePhotosCounselingCenter(!seePhotosCounselingCenter)}} className=\"see-photos-button\">\n                                {seePhotosFamilyDay ? \"x close photos\" : \"> see photos\"}\n                            </button>\n                        </div>\n                            {\n                                seePhotosCounselingCenter &&\n                                    <div className='photos'>\n                                        <CounselingCenter1 className='collection-image'/>\n                                        <CounselingCenter2 className='collection-image'/>\n                                        <CounselingCenter3 className='collection-image'/>\n                                    </div>\n                            }\n\n\n\n\n\n\n\n\n                    </div>\n                </div>\n               \n                <Footer/>   \n            </div>\n        </div>\n    )\n}\n\nexport default ProyectoPlay;","/Users/diegolaya/Desktop/Projects/theraspanishplay/theraspanishplay/src/components/home/Home.js",["152","153"],[],"import { React, useEffect } from 'react';\nimport { Link, useHistory } from 'react-router-dom';\n\nimport '../../App.css';\nimport './Home.css'\nimport { ReactComponent as Logo } from '../../assets/navigation-assets/Logo.svg';\nimport { ReactComponent as PrivateClasses } from '../../assets/navigation-assets/PrivateClasses.svg';\nimport { ReactComponent as ProyectoPlay } from '../../assets/navigation-assets/ProyectoPlay.svg';\nimport { ReactComponent as Illustration } from '../../assets/navigation-assets/Illustration.svg';\nimport { ReactComponent as Rectangle } from '../../assets/navigation-assets/Rectangle.svg';\nimport { ReactComponent as Ovalo } from '../../assets/navigation-assets/Ovalo.svg';\nimport { ReactComponent as Rayon } from '../../assets/navigation-assets/Rayon.svg';\nimport Footer from '../footer/Footer.js';\nimport $ from 'jquery';\n\nconst Home = () => {\n\n    useEffect(() => {\n        $('div.portion').css('height', $('div.portion').height());\n        $('div.portion').css('width', $('div.portion').width());\n    });\n\n    return (\n        <div>\n            <div className='cream portion'>\n                <div id=\"slogan\">\n                    <p className='huge-title'><span className='teal-text'>PLAY</span> IS NOT A BREAK FROM LEARNING.</p>\n                    <p className='huge-title'>IT'S HOW WE <span className='teal-text'>LEARN.</span></p>\n                </div>\n                <div id=\"buttons-under-slogan\">\n                    <button class=\"button purple\">\n                        <Link to=\"/program\"  title=\"Home\">\n                            <p className='black-text'>see our program</p>\n                        </Link>\n                    </button>\n                    <button id=\"get-to-know-us\" className=\"button green\">\n                        <Link to=\"/about\"  title=\"Home\">\n                            <p className='black-text'>get to know us</p>\n                        </Link>\n                    </button>\n                </div>\n                <Illustration id=\"illustration\"/>\n            </div>\n            <div id=\"brief\" className='teal'>\n                <div className='info-portion'>\n                    <div id=\"top-section\" className='info-section'>\n                        <PrivateClasses className='boxy-image'/>\n                        <button className='button orange'>\n                            <p>sign up</p>\n                        </button>\n                        <p className=\"paragraph\">\n                        Play is an essential part of every child’s life. TheraSpanishPlay\n                        uses a unique teaching method of high quality planned experiences\n                        to teach kids Spanish, while also enhacing positive relationships\n                        and self-esteem.\n                        </p>\n                    </div>\n                    <div id=\"bottom-section\" className='info-section'>\n                        <ProyectoPlay className='boxy-image'/>\n                        <button  onClick={()  => { window.open('mailto:theraspanishplay@gmail.com?subject=Partner%20Request&body=')}} className='button dark-green'>\n                            <p>partner with us</p>\n                        </button>\n                        <p id=\"partner-text\" className=\"paragraph\">\n                        Proyecto Play focuses on hosting large scale community events for\n                        parents and children to get together, and play traditional games\n                        spending quality time free of any electronic distraction.\n                        </p>\n                    </div>\n                </div>\n            </div>\n            <div id='reviews' className='cream'>\n                <div id=\"container\">\n                    <Rectangle id=\"rectangle\"/>\n                    <p id=\"paragraph-one\">\n                        “We have been fortunate to work with Ms. Ileana for the past couple of years.\n                        She’s very creative and has an exciting style that makes learning Spanish fun.\n                        The children look forward to the class always, due to her commitment, joy and\n                        passion in learning the language and in that they have been able to progress\n                        very well.”\n                        <h3>- Mona Seraj Saleh</h3>\n                    </p>\n                    <Ovalo id=\"ovalo\"/>\n                    <p id=\"paragraph-two\">\n                        “Ms. Ileana is a wonderful teacher.  My daughter has been with her since she\n                        was 3 years old, and now she is 9 years old and fluent in Spanish. She looks\n                        forward to every lesson as they are play-based. I remember once, three years\n                        ago, we were in Switzerland and my daughter talked to a waiter in English. He  \n                        didn’t understand, so she switched to Spanish and he understood! I’m so proud\n                        of her and so happy that we have chosen Ms. Ileana.”\n                        <h3>- Aysha</h3>\n                    </p>\n                    <Rayon id=\"rayon\"/>\n                    <p id=\"paragraph-three\">\n                        “I have been having Spanish lessons with Ileana for several enjoyable years.\n                        As an older person, who had not undertaken any formal study for many years,\n                        I was afraid I would be unable to cope with weekly classes. However, Ileana’s\n                        professional approach, with each lesson tailored to the student’s individual\n                        needs and development, has ensured my steady progress and nurtured my love for\n                        the Spanish language. I am pleased to say that now my son is also taking lessons\n                        with Ileana.”\n                        <h3>- Madeleine Gurney</h3>\n                    </p>\n                </div>\n            </div>\n            <div className=\"bright-yellow portion\">\n                <div id=\"follow-us-container\">\n                    <h2>Follow Us</h2>\n                    <h1>@THERASPANISHPLAY</h1>\n                </div>\n            </div>\n            <div className='unconstrained-portion bright-yellow'>\n                <div style={{'width':'90%'}}>\n                    <Footer/>\n                </div>\n            </div>\n        </div>\n    );\n}\n\nexport default Home;","/Users/diegolaya/Desktop/Projects/theraspanishplay/theraspanishplay/src/components/navigation/Navigation.js",["154"],[],"import { Link, useHistory } from 'react-router-dom';\nimport { ReactComponent as Logo } from '../../assets/navigation-assets/Logo.svg';\nimport { ReactComponent as BurguerMenu } from '../../assets/navigation-assets/BurgerMenu.svg';\nimport \"./Navigation.css\"\nimport \"../../App.css\"\n\nconst Navigation = (toggleMenu) => {\n    return (\n        <div id=\"navbar\">\n            <Link id=\"link-logo\" to=\"/home\"  title=\"Home\">\n                <Logo id=\"logo\"/>\n            </Link>\n           <div id=\"burger-and-contact\">\n                <BurguerMenu onClick={toggleMenu.toggleMenu} id=\"burger-menu\"/>\n                <button class=\"button gray\">\n                    <p id=\"contact-us-text\">\n                        contact us\n                    </p>\n                </button>\n           </div>\n        </div>\n    );\n\n}\n\nexport default Navigation;","/Users/diegolaya/Desktop/Projects/Website/src/index.js",[],[],"/Users/diegolaya/Desktop/Projects/Website/src/App.js",["155"],[],"/Users/diegolaya/Desktop/Projects/Website/src/reportWebVitals.js",[],[],"/Users/diegolaya/Desktop/Projects/Website/src/choreograph/Choreograph.js",["156","157"],[],"/Users/diegolaya/Desktop/Projects/Website/src/components/menu/Menu.js",["158"],[],"/Users/diegolaya/Desktop/Projects/Website/src/components/navigation/Navigation.js",["159"],[],"/Users/diegolaya/Desktop/Projects/Website/src/components/home/Home.js",["160","161"],[],"/Users/diegolaya/Desktop/Projects/Website/src/components/footer/Footer.js",["162","163"],[],"/Users/diegolaya/Desktop/Projects/TheraSpanishPlay-Website/src/index.js",[],[],"/Users/diegolaya/Desktop/Projects/TheraSpanishPlay-Website/src/App.js",["164"],[],"/Users/diegolaya/Desktop/Projects/TheraSpanishPlay-Website/src/reportWebVitals.js",[],[],"/Users/diegolaya/Desktop/Projects/TheraSpanishPlay-Website/src/choreograph/Choreograph.js",["165","166"],[],"import React, { useState, useEffect } from 'react';\nimport { Switch, Route } from 'react-router-dom';\n\nimport Home from '../components/home/Home.js'\n\nconst Choreograph = () => {\n    return (\n        <Switch>\n            <Route exact path=\"/\" render={() => <Home />} />\n        </Switch>\n    );\n\n}\n\nexport default Choreograph;","/Users/diegolaya/Desktop/Projects/TheraSpanishPlay-Website/src/components/navigation/Navigation.js",["167"],[],"/Users/diegolaya/Desktop/Projects/TheraSpanishPlay-Website/src/components/menu/Menu.js",["168"],[],"/Users/diegolaya/Desktop/Projects/TheraSpanishPlay-Website/src/components/home/Home.js",["169","170"],[],"/Users/diegolaya/Desktop/Projects/TheraSpanishPlay-Website/src/components/footer/Footer.js",["171","172"],[],{"ruleId":"173","sever